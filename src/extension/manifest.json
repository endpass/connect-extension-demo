{
  "name": "connect-extension-demo",
  "version": "1.0",
  "description": "Connect extension demo",
  "manifest_version": 2,
  "permissions": ["tabs", "https://*/*"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": ["injected.js", "browser.js"],
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ]
}
